<!DOCTYPE html>
<html lang="en">
<head>
    <title>All Users</title>
</head>
    <body>
        <h1>These are all of the users...</h1>
        <ul>
            <% users.forEach( function(user) { %>
            <li> <%= user.firstName %>  <%= user.lastName %>: Age:<%= user.age %>, Email: <%= user.email %>  </li>
           <% }) %>
        </ul>
    </body>
</html>